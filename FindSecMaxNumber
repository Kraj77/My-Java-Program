class demo{
    static int PrintSecMax(int n,int a[]){
        int max= Integer.MIN_VALUE;
        int sec_max=Integer.MIN_VALUE;
        for(int i=0;i<n;i++){
            if(a[i]>max){
                sec_max=max;
                max=a[i];
            } else if(a[i]>sec_max){
                sec_max= a[i];
            }

        }
        return sec_max;
    }
    public static void main(String []args){
        System.out.println(PrintSecMax(7, new int[]{1, 2, 3, 4, 5,26, 17 }));
    }
}
