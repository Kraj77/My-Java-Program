import java.util.*;
class ArraySecMax{
    public static void main(String[] args) {
        int a[] = {10,20,20,30,40,50};
       boolean ans= sorted(a);
        System.out.println(ans);

    }

    public static boolean sorted(int []a){
        boolean bool = true;
      for(int i=1;i<a.length;i++){
          if (a[i]<a[i-1]){
              bool = false;
          }
      }
      return bool;
    }
    }
