class demo{
    static boolean isPalindrome(String name){

        int length= name.length();
        for(int i=0;i<length/2;i++){
            if(name.charAt(i)!=name.charAt(length-i-1)){
                return false;
            }
        }
        return true;
    }
    public static void main(String []args){
        System.out.println(isPalindrome("ABCDCBA"));
    }
}
