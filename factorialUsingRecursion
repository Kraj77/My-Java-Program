import java.util.*;
class factorialUsingRecursion{
    public static int factorial(int x){
        if(x==1 || x<=0){
            return 1;
        }
        else {
            return x*(factorial(x-1));
        }
    }
    public static void main(String []args){
        Scanner sc =new Scanner(System.in);
        int n =sc.nextInt();
        System.out.println(factorial(n));
    }
}
